{
  "extends": ["next/core-web-vitals", "eslint:recommended", "airbnb"],
  "rules": {
    "jsx-quotes": ["error", "prefer-double"],
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": "off"
  }
}
